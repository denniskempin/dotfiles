[
    // cmd + [ and ] navigates back and forth, like in safari/chrome.
    {
        "key": "cmd+[",
        "command": "workbench.action.navigateBack"
    },
    {
        "key": "cmd+]",
        "command": "workbench.action.navigateForward"
    },

    // Use Code36 keyboard bindings for commands / quick open
    {
        "key": "ctrl+cmd+space",
        "command": "workbench.action.showCommands"
    },
    {
        "key": "shift+cmd+space",
        "command": "workbench.action.quickOpen"
    }
]